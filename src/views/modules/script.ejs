<script>
    //let text = document.getElementById("shortUrl").addEventListener("click", copy);

    let text = document.getElementById('shortUrl').innerHTML;

    const copy = async () => {
        try {
            await navigator.clipboard.writeText(text);
            console.log('Content copied to clipboard');
        } catch (err) {
            console.error('Failed to copy: ', err);
        }
    }
</script>

